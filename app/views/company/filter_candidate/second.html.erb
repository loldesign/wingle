<section id="second_step" class="filter-container content">
  <section class="content-box">
    <ul class="collection with-header">
      <li class="collection-header">
        <div class="progress">
          <div class="determinate" style="width: 40%"></div>
        </div>
        <h5 class="center-align section"></h5>
      </li>

      <% @function.each do |function| %>
        <li class="collection-item">
          <%= link_to function.name, company_filter_step_3_path %>
        </li>
      <% end %>
    </ul>
  </section>

  <section class="action-buttons-single">
    <%= link_to "Anterior", company_filter_step_1_path, class: "waves-effect waves-light btn-large" %>
  </section>
</section>